{
  "name": "tle-parser-grpc-api",
  "version": "1.0.0",
  "description": "gRPC API for TLE Parser",
  "main": "server.ts",
  "scripts": {
    "dev": "ts-node server.ts",
    "start": "node dist/server.js",
    "build": "tsc",
    "proto:generate": "grpc_tools_node_protoc --js_out=import_style=commonjs,binary:./generated --grpc_out=grpc_js:./generated --plugin=protoc-gen-grpc=`which grpc_tools_node_protoc_plugin` tle-parser.proto",
    "test": "jest"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.9.0",
    "@grpc/proto-loader": "^0.7.10"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "grpc-tools": "^1.12.4",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.0"
  }
}
